{
  "name": "pea-shootin-pete",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:ai": "EXPO_PUBLIC_AI_MODE=true expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:ai": "EXPO_PUBLIC_AI_MODE=true expo run:android",
    "ios": "expo run:ios",
    "ios:ai": "EXPO_PUBLIC_AI_MODE=true expo run:ios",
    "web": "expo start --web",
    "web:ai": "EXPO_PUBLIC_AI_MODE=true expo start --web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "npx tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "test": "jest testing/tests/",
    "test:watch": "jest testing/tests/ --watch",
    "test:coverage": "jest testing/tests/ --coverage",
    "test:performance": "jest testing/tests/ --testNamePattern=\"Performance\"",
    "test:gameplay": "jest testing/tests/basic-gameplay.test.ts",
    "test:ai": "EXPO_PUBLIC_AI_MODE=true npm run test:gameplay"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.10",
    "expo-av": "~14.0.6",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.8",
    "expo-web-browser": "~14.1.6",
    "nanoid": "^5.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "typescript": "~5.8.3",
    "ts-node": "^10.9.2"
  },
  "private": true
}